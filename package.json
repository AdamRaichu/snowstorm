{
	"name": "snowstorm",
	"version": "2.6.1",
	"description": "Minecraft Bedrock Edition particle editor",
	"author": {
		"name": "JannisX11",
		"email": "info@blockbench.net"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JannisX11/snowstorm"
	},
	"license": "GPL-3.0-or-later",
	"scripts": {
		"dev": "npm run development && npm run copy-extension",
		"development": "mix",
		"watch": "mix watch",
		"watch-poll": "mix watch -- --watch-options-poll=1000",
		"hot": "mix watch --hot",
		"production": "mix --production && npm run copy-extension",
		"copy-extension": "copy .\\dist\\app.js .\\vscode_extension\\snowstorm\\app.js",
		"extension": "npm run dev && npm run copy-extension",
		"build-extension": "cd ./vscode_extension && vsce package"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"laravel-mix": "^6.0.49",
		"postcss": "^8.4.12",
		"sass": "^1.30.0",
		"sass-loader": "^8.0.2",
		"url-loader": "^4.1.1",
		"vue-loader": "15.9.8",
		"vue-template-compiler": "^2.6.12",
		"webpack": "^5.89.0",
		"webpack-bundle-size-analyzer": "^3.1.0",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"bootstrap": "^4.5.3",
		"fflate": "^0.7.3",
		"lucide-vue": "^0.298.0",
		"molangjs": "^1.6.2",
		"prismjs": "^1.28.0",
		"root": "github:JannisX11/vue-prism-editor",
		"three": "^0.129.0",
		"vue": "^2.6.12",
		"vue-color": "^2.8.0",
		"vue-prism-component": "^1.2.0",
		"wintersky": "^1.2.1"
	}
}
